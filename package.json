{
	"name": "chat-sample",
	"publisher": "vscode-samples",
	"displayName": "Copilot Chat Sample",
	"description": "Sample chat extension, a trusty cat tutor that will can teach you computer science topics.",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.92.0"
	},
	"categories": [
		"AI",
		"Chat"
	],
	"activationEvents": [],
	"contributes": {
		"chatParticipants": [
			{
				"id": "chat-sample.cppcon",
				"fullName": "CppCon",
				"name": "cppcon",
				"description": "Welcome to CppCon! Ask me any questions about the schedule for the conference!",
				"isSticky": true,
				"commands": [
					{
						"name": "talks",
						"description": "Get the list of talks for the conference"
					}
				]

			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"dependencies": {
		"@vscode/prompt-tsx": "^0.2.3-alpha"
	},
	"devDependencies": {
		"@types/node": "^20.5.9",
		"@types/vscode": "1.90.0",
		"@typescript-eslint/eslint-plugin": "^7.14.0",
		"@typescript-eslint/parser": "^7.14.0",
		"eslint": "^8.26.0",
		"typescript": "^5.5.2"
	}
}